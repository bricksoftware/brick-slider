{
  "semi": false,
  "singleQuote": false,
  "arrowParens": "avoid",
  "trailingComma": "none",
  "printWidth": 100,
  
  "overrides": [
    {
      "files": "*.html",
      "options": {
        "parser": "html"
      }
    },
    {
      "files": "*.css",
      "options": {
        "parser": "css"
      }
    }
  ]
}





